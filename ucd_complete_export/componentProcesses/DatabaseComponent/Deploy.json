{
  "name": "Deploy",
  "type": "COMPONENT",
  "description": "Liquibase update",
  "steps": [
    {
      "name": "Liquibase Update",
      "plugin": "Liquibase",
      "command": "Update",
      "properties": {
        "changeLogFile": "${p:DatabaseComponent/changeLogFile}",
        "url": "${p:DatabaseComponent/jdbcUrl}",
        "username": "${p:DatabaseComponent/dbUser}",
        "password": "${p:?DatabaseComponent/dbPassword}"
      }
    }
  ]
}